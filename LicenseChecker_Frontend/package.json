    "core-js": "^3.48.0",
    "element-plus": "^2.13.1",
    "http-proxy-middleware": "^3.0.5",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "vue-router": "^5.0.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.10.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.1.0",
