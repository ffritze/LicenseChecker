    "jsdom": "^27.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
